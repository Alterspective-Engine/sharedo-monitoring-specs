# Example environment configuration file
# Copy this to .env and fill in your actual values

SLACK_TOKEN=your_slack_token_here
CHANNEL_ID=your_channel_id_here
SLACK_COOKIES=your_slack_cookies_here

REDIS_URL=redis://localhost:6379
TEST_RUN=0

# Azure OpenAI Configuration
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_DEPLOYMENT=your-deployment-name
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here

# OpenAI Configuration
OPENAI_API_KEY=sk-proj-your_openai_api_key_here

# Azure Configuration
AZURE_SUBSCRIPTION_ID=your_subscription_id_here
AZURE_RESOURCE_GROUP=your_resource_group_here
AZURE_OPENAI_RESOURCE=your_openai_resource_here
AZURE_CLIENT_ID=your_client_id_here
AZURE_CLIENT_SECRET=your_client_secret_here
AZURE_TENANT_ID=your_tenant_id_here

# Hugging Face Configuration
HUGGINGFACE_API_KEY=hf_your_huggingface_api_key_here

# ShareDo API Configuration
SHAREDO_API_BASE_URL=https://your-environment.sharedo.tech/
SHAREDO_IDENTITY_SERVER_URL=https://your-identity-server.sharedo.tech/connect/token
SHAREDO_CLIENT_USERNAME=your_client_username
SHAREDO_CLIENT_PASSWORD=your_client_password
SHAREDO_GRANT_TYPE=Impersonate.Specified
SHAREDO_SCOPE=sharedo
SHAREDO_TOKEN_EXPIRATION_SECONDS=600
SHAREDO_IMPERSONATE_USER=your_email@example.com
SHAREDO_IMPERSONATE_PROVIDER=sharedo-aad

# Supabase Configuration
SUPABASE_URL=http://localhost:8000
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
POSTGRES_PASSWORD=your_postgres_password_here
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRY=3600
ANON_KEY=your_anon_key_here
SERVICE_ROLE_KEY=your_service_role_key_here
PUBLIC_REST_URL=http://localhost:8000/rest/v1/
STUDIO_DEFAULT_ORGANIZATION=Default Organization
STUDIO_DEFAULT_PROJECT=Enhanced Log Analysis
DOCKER_SOCKET_LOCATION=/var/run/docker.sock
IMGPROXY_ENABLE_WEBP_DETECTION=false